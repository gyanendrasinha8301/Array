#include <iostream>
#include <stdio.h>

using namespace std;

int main()
{
    int n;
    int arr[n];
    cout<<"Enter the Size of Array: ";
    cin>>n;
    cout<<"\nEnter the Array Elements: ";
    for(int i=1; i<=n; i++)
        {cin>>arr[i];}
    int *p=&arr[n];
    cout<<"\nThe Array Elements are: ";
    for(int p=1; p<=n; p++)
        {cout<<arr[p]<<"\n";}
    return 0;
}
